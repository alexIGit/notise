#!/bin/bash

echo "Без использования js"
let delay=1
let countBower=0
let itogoBower=1

echo "-----------"
echo "Сначала bower"
echo "bower init"
sleep $delay
bower init
echo "Установка bower модулей в src"
sleep $delay
cd src
echo "-----------"
echo "$((++countBower)) из $itogoBower"
echo "neat"
sleep $delay
bower i -S neat
